{
    "name": "metrixinfo/eloquent-sortable",
    "description": "Sortable Trait for Laravel Eloquent models that supports grouping on multiple keys",
    "homepage": "https://github.com/metrixinfo/eloquent-sortable",
    "authors": [
        {
            "name": "Michael J. Love",
            "email": "info@metrixinfo.com"
        }
    ],
    "keywords":
        [
            "sort",
            "order",
            "sortable",
            "eloquent",
            "model",
            "laravel",
            "trait",
            "display order"
        ],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "consistence/coding-standard": "^2.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/php-code-coverage": "^5.2",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Metrix\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metrix\\EloquentSortable\\Test\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.15"
    }
}
